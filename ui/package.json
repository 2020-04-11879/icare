{
  "name": "icare-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --aot --configuration production",
    "test": "ng test",
    "test:headless": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "concurrently  -s \"first\" --kill-others \"ng serve\" \"cypress-tags run -e TAGS='@focus'\"",
    "e2e:headed": "concurrently  -s \"first\" --kill-others \"ng serve\" \"cypress-tags run -e TAGS='@focus' --headed\"",
    "e2e:interactive": "concurrently \"ng serve\" \"cypress open\"",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test:e2e": "npm run clean:reports && npm run e2e || npm run posttest",
    "generate-api": "curl 'http://icare.co.tz/openmrs/module/webservices/rest/swagger.json' > ./swagger.json && sed -i 's/parent-uuid/parentUuid/g' swagger.json && swagger-typescript-api -p ./swagger.json -o ./ -n ./src/app/shared/modules/openmrs.ts",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^9.0.0",
    "@angular/animations": "~15.0.2",
    "@angular/cdk": "^15.0.1",
    "@angular/common": "~15.0.2",
    "@angular/compiler": "~15.0.2",
    "@angular/core": "^15.0.2",
    "@angular/forms": "~15.0.2",
    "@angular/material": "^15.0.1",
    "@angular/material-moment-adapter": "^15.0.1",
    "@angular/platform-browser": "~15.0.2",
    "@angular/platform-browser-dynamic": "~15.0.2",
    "@angular/router": "~15.0.2",
    "@ngrx/effects": "^15.0.0",
    "@ngrx/entity": "^15.0.0",
    "@ngrx/router-store": "^15.0.0",
    "@ngrx/store": "^15.0.0",
    "@ngrx/store-devtools": "^15.0.0",
    "bootstrap": "^5.2.3",
    "cypress": "^11.2.0",
    "dexie": "^3.2.2",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "highcharts": "^10.3.2",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.4",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-barcode": "^0.3.0",
    "ngx-print": "^1.2.1",
    "num-words": "^1.2.2",
    "quick-pivot": "^2.7.0",
    "rxjs": "~7.6.0",
    "tslib": "^2.4.1",
    "xlsx": "^0.18.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "^15.0.2",
    "@angular/compiler-cli": "~15.0.2",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.11.10",
    "codelyzer": "^0.0.28",
    "commitizen": "^4.2.5",
    "concurrently": "^7.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.2",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "mocha": "^10.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "protractor": "~7.0.0",
    "swagger-typescript-api": "^12.0.2",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
